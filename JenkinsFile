pipeline{
 
 agent any
 tools{
	maven 'M3'
 	jdk 'jdk_1.8.0_151'
 }
 stages{
 
	stage('Build'){
	
		steps{
			echo "Building Project"
			bat "mvn clean"
		}
	}
	stage('Test'){
	
		steps{
			echo "Building Project"
			bat "mvn test"
		}
	}
	stage('Deploy'){
	
		steps{
			echo "Building Project"
			bat "mvn verify"
		}
	}
	
 
 
 
 
 }



}